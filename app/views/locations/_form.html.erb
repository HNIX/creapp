<%= simple_form_for(@location) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :address %>
    <%= f.input :longitude %>
    <%= f.input :latitude %>
    <%= f.association :user %>
    <%= f.association :company %>
    <%= f.association :asset %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
